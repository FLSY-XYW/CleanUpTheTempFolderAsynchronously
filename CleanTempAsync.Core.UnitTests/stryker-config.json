{
  "stryker-config": {
    "project": "CleanTempAsync.Core.csproj",
    "reporters": [
      "cleartexttree",
      "html"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 30
    },
    "mutate": [
      "!Program.cs",
      "!**/*DbContext.cs",
      "!**/Repositories/*.cs",
      "!**/Migrations/*.cs",
      "!**/EntityConfigs/*.cs",
      "!*Exception.cs"
    ]
  }
}